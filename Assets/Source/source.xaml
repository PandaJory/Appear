<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:System="clr-namespace:System;assembly=mscorlib">
    <x:Array x:Key="DockPositions" Type="sys:String"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:sys="clr-namespace:System;assembly=mscorlib">
        <sys:String>Top</sys:String>
        <sys:String>Bottom</sys:String>
        <!--<sys:String>Left</sys:String>
        <sys:String>Right</sys:String>-->
    </x:Array>

    <x:Array x:Key="Styles" Type="sys:String"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:sys="clr-namespace:System;assembly=mscorlib">
        <sys:String>Default</sys:String>
        <sys:String>Eggs</sys:String>
        <sys:String>Nebula</sys:String>
        <sys:String>Sky</sys:String>
    </x:Array>

    <x:Array x:Key="Modes" Type="sys:String"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:sys="clr-namespace:System;assembly=mscorlib">
        <sys:String>MANUAL</sys:String>
        <sys:String>AUTO PLAY</sys:String>
    </x:Array>
</ResourceDictionary>